@using ReadTheNews.Models
@model List<RssItem>
@{
    ViewBag.Title = "Список для чтения";
    ViewBag.ReadItLaterActive = "active";
}

<hr />
@foreach (RssItem item in Model)
{
    <div class="gray-bg">
        @Html.Partial("SingleNews", item)
    </div>
}
<div class="ibox" style="height:0; opacity:0;"></div>